<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.omniperform.system.mapper.DashboardMemberStageMapper">
    
    <resultMap type="DashboardMemberStage" id="DashboardMemberStageResult">
        <result property="id"    column="id"    />
        <result property="dataMonth"    column="data_month"    />
        <result property="stageName"    column="stage_name"    />
        <result property="memberCount"    column="member_count"    />
        <result property="percentage"    column="percentage"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectDashboardMemberStageVo">
        select id, data_month, stage_name, member_count, percentage, create_by, create_time, update_by, update_time, remark from dashboard_member_stage
    </sql>

    <select id="selectDashboardMemberStageList" parameterType="DashboardMemberStage" resultMap="DashboardMemberStageResult">
        <include refid="selectDashboardMemberStageVo"/>
        <where>  
            <if test="dataMonth != null  and dataMonth != ''"> and data_month = #{dataMonth}</if>
            <if test="stageName != null  and stageName != ''"> and stage_name = #{stageName}</if>
            <if test="memberCount != null "> and member_count = #{memberCount}</if>
        </where>
        order by data_month desc
    </select>
    
    <select id="selectDashboardMemberStageById" parameterType="Long" resultMap="DashboardMemberStageResult">
        <include refid="selectDashboardMemberStageVo"/>
        where id = #{id}
    </select>

    <select id="selectByDataMonth" parameterType="String" resultMap="DashboardMemberStageResult">
        <include refid="selectDashboardMemberStageVo"/>
        where data_month = #{dataMonth}
    </select>

    <select id="selectByMonthRange" resultMap="DashboardMemberStageResult">
        <include refid="selectDashboardMemberStageVo"/>
        where data_month between #{startMonth} and #{endMonth}
        order by data_month asc
    </select>
        
    <insert id="insertDashboardMemberStage" parameterType="DashboardMemberStage" useGeneratedKeys="true" keyProperty="id">
        insert into dashboard_member_stage
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">data_month,</if>
            <if test="newMemberCount != null">new_member_count,</if>
            <if test="activeMemberCount != null">active_member_count,</if>
            <if test="dormantMemberCount != null">dormant_member_count,</if>
            <if test="lostMemberCount != null">lost_member_count,</if>
            <if test="newMemberPercentage != null">new_member_percentage,</if>
            <if test="activeMemberPercentage != null">active_member_percentage,</if>
            <if test="dormantMemberPercentage != null">dormant_member_percentage,</if>
            <if test="lostMemberPercentage != null">lost_member_percentage,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">#{dataMonth},</if>
            <if test="newMemberCount != null">#{newMemberCount},</if>
            <if test="activeMemberCount != null">#{activeMemberCount},</if>
            <if test="dormantMemberCount != null">#{dormantMemberCount},</if>
            <if test="lostMemberCount != null">#{lostMemberCount},</if>
            <if test="newMemberPercentage != null">#{newMemberPercentage},</if>
            <if test="activeMemberPercentage != null">#{activeMemberPercentage},</if>
            <if test="dormantMemberPercentage != null">#{dormantMemberPercentage},</if>
            <if test="lostMemberPercentage != null">#{lostMemberPercentage},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateDashboardMemberStage" parameterType="DashboardMemberStage">
        update dashboard_member_stage
        <trim prefix="SET" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">data_month = #{dataMonth},</if>
            <if test="newMemberCount != null">new_member_count = #{newMemberCount},</if>
            <if test="activeMemberCount != null">active_member_count = #{activeMemberCount},</if>
            <if test="dormantMemberCount != null">dormant_member_count = #{dormantMemberCount},</if>
            <if test="lostMemberCount != null">lost_member_count = #{lostMemberCount},</if>
            <if test="newMemberPercentage != null">new_member_percentage = #{newMemberPercentage},</if>
            <if test="activeMemberPercentage != null">active_member_percentage = #{activeMemberPercentage},</if>
            <if test="dormantMemberPercentage != null">dormant_member_percentage = #{dormantMemberPercentage},</if>
            <if test="lostMemberPercentage != null">lost_member_percentage = #{lostMemberPercentage},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDashboardMemberStageById" parameterType="Long">
        delete from dashboard_member_stage where id = #{id}
    </delete>

    <delete id="deleteDashboardMemberStageByIds" parameterType="String">
        delete from dashboard_member_stage where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>