<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.omniperform.system.mapper.MemberCrfmeDistributionMapper">
    
    <resultMap type="MemberCrfmeDistribution" id="MemberCrfmeDistributionResult">
        <result property="id"    column="id"    />
        <result property="dataMonth"    column="data_month"    />
        <result property="scoreRange"    column="score_range"    />
        <result property="count"    column="count"    />
        <result property="percentage"    column="percentage"    />
        <result property="avgScore"    column="avg_score"    />
        <result property="tier"    column="tier"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectMemberCrfmeDistributionVo">
        select id, data_month, score_range, count, percentage, avg_score, tier, create_by, create_time, update_by, update_time, remark from member_crfme_distribution
    </sql>

    <select id="selectMemberCrfmeDistributionList" parameterType="MemberCrfmeDistribution" resultMap="MemberCrfmeDistributionResult">
        <include refid="selectMemberCrfmeDistributionVo"/>
        <where>  
            <if test="dataMonth != null  and dataMonth != ''"> and data_month = #{dataMonth}</if>
            <if test="scoreRange != null  and scoreRange != ''"> and score_range = #{scoreRange}</if>
            <if test="tier != null  and tier != ''"> and tier = #{tier}</if>
        </where>
        order by data_month desc, avg_score desc
    </select>
    
    <select id="selectMemberCrfmeDistributionById" parameterType="Long" resultMap="MemberCrfmeDistributionResult">
        <include refid="selectMemberCrfmeDistributionVo"/>
        where id = #{id}
    </select>

    <select id="selectByDataMonth" parameterType="String" resultMap="MemberCrfmeDistributionResult">
        <include refid="selectMemberCrfmeDistributionVo"/>
        where data_month = #{dataMonth}
        order by avg_score desc
    </select>

    <select id="selectByMonthRange" resultMap="MemberCrfmeDistributionResult">
        <include refid="selectMemberCrfmeDistributionVo"/>
        where data_month between #{startMonth} and #{endMonth}
        order by data_month asc, avg_score desc
    </select>
        
    <insert id="insertMemberCrfmeDistribution" parameterType="MemberCrfmeDistribution" useGeneratedKeys="true" keyProperty="id">
        insert into member_crfme_distribution
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">data_month,</if>
            <if test="scoreRange != null and scoreRange != ''">score_range,</if>
            <if test="count != null">count,</if>
            <if test="percentage != null">percentage,</if>
            <if test="avgScore != null">avg_score,</if>
            <if test="tier != null and tier != ''">tier,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">#{dataMonth},</if>
            <if test="scoreRange != null and scoreRange != ''">#{scoreRange},</if>
            <if test="count != null">#{count},</if>
            <if test="percentage != null">#{percentage},</if>
            <if test="avgScore != null">#{avgScore},</if>
            <if test="tier != null and tier != ''">#{tier},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateMemberCrfmeDistribution" parameterType="MemberCrfmeDistribution">
        update member_crfme_distribution
        <trim prefix="SET" suffixOverrides=",">
            <if test="dataMonth != null and dataMonth != ''">data_month = #{dataMonth},</if>
            <if test="scoreRange != null and scoreRange != ''">score_range = #{scoreRange},</if>
            <if test="count != null">count = #{count},</if>
            <if test="percentage != null">percentage = #{percentage},</if>
            <if test="avgScore != null">avg_score = #{avgScore},</if>
            <if test="tier != null and tier != ''">tier = #{tier},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteMemberCrfmeDistributionById" parameterType="Long">
        delete from member_crfme_distribution where id = #{id}
    </delete>

    <delete id="deleteMemberCrfmeDistributionByIds" parameterType="String">
        delete from member_crfme_distribution where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>